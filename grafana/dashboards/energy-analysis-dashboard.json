        {
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH energy_analysis AS (\n    SELECT \n        SUM(CASE WHEN k.execution_space = 'Cuda' THEN k.energy_joules ELSE 0 END) AS cuda_energy,\n        SUM(CASE WHEN k.execution_space != 'Cuda' THEN k.energy_joules ELSE 0 END) AS wasted_gpu_energy,\n        SUM(k.energy_joules) AS total_kernel_energy\n    FROM kernels_info k\n)\nSELECT \n    wasted_gpu_energy AS \"GPU Wasted Energy (J)\",\n    cuda_energy AS \"Useful GPU Energy (J)\",\n    CAST(\n        (wasted_gpu_energy * 100.0 / (cuda_energy + wasted_gpu_energy))\n        AS DECIMAL(5,1)\n    ) AS \"GPU Energy Waste (%)\"\nFROM energy_analysis;",
          "refId": "A",
