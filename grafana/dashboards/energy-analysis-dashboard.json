      "targets": [
        {
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH unknown_measurements AS (\n    SELECT \n        p.timestamp_ms,\n        p.power_watts,\n        CASE \n            WHEN p.timestamp_ms BETWEEN \n                (SELECT MIN(start_time_ms) FROM kernels_info) \n                AND \n                (SELECT MAX(end_time_ms) FROM kernels_info)\n            THEN 20  -- Measurement interval in ms\n            ELSE 20\n        END as measurement_interval_ms\n    FROM power_over_time p\n    LEFT JOIN kernels_info k ON (\n        p.device_id = k.device_id \n        AND p.timestamp_ms >= k.start_time_ms \n        AND p.timestamp_ms <= k.end_time_ms\n    )\n    WHERE k.id IS NULL  -- Only measurements without associated kernel\n)\nSELECT \n    COUNT(*) AS \"Number of measurements\",\n    CAST(AVG(power_watts) AS DECIMAL(10,2)) AS \"Average power (W)\",\n    CAST(SUM(power_watts * measurement_interval_ms / 1000.0) AS DECIMAL(10,3)) AS \"Estimated lost energy (J)\",\n    CAST(COUNT(*) * 20 / 1000.0 AS DECIMAL(10,2)) AS \"Total time (s)\"\nFROM unknown_measurements;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "Quick Info - Power Measurement No Kernels",
      "type": "stat"
