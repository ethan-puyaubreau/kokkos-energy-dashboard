      "targets": [
        {
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH energy_breakdown AS (\n    -- CUDA energy (source directe - la plus fiable)\n    SELECT \n        'CUDA Kernels' AS category,\n        SUM(k.energy_joules) AS energy_joules,\n        'Direct' AS source_type\n    FROM kernels_info k\n    WHERE k.execution_space = 'Cuda'\n    \n    UNION ALL\n    \n    -- Non-CUDA kernels energy (source directe - fiable)\n    SELECT \n        'Non-CUDA Kernels' AS category,\n        SUM(k.energy_joules) AS energy_joules,\n        'Direct' AS source_type\n    FROM kernels_info k\n    WHERE k.execution_space != 'Cuda'\n    \n    UNION ALL\n    \n    -- Outside kernels (approximation nécessaire)\n    SELECT \n        'Outside Kernels' AS category,\n        CAST(SUM(p.power_watts * 20 / 1000.0) AS DECIMAL(10,3)) AS energy_joules,\n        'Approximated (±20ms)' AS source_type\n    FROM power_over_time p\n    LEFT JOIN kernels_info k ON (\n        p.device_id = k.device_id \n        AND p.timestamp_ms >= k.start_time_ms \n        AND p.timestamp_ms <= k.end_time_ms\n    )\n    WHERE k.id IS NULL\n)\nSELECT \n    category AS \"Energy Category\",\n    energy_joules AS \"Energy (J)\",\n    source_type AS \"Data Quality\",\n    -- Calcul de la marge d'erreur\n    CASE \n        WHEN source_type = 'Approximated (±20ms)' \n        THEN CAST(energy_joules * 0.02 AS DECIMAL(8,3))  -- ~2% d'erreur estimée\n        ELSE 0 \n    END AS \"Est. Error (J)\"\nFROM energy_breakdown\nORDER BY \"Energy (J)\" DESC;",
